<h1>Listing contacts</h1>

<table>
  <tr>
    <th>Contact type</th>
    <th>Contact data</th>
    <th>Created userid</th>
    <th>Last updt userid</th>
  </tr>

<% @contacts.each do |contact| %>
  <tr>
    <td><%=h contact.contact_type_id %></td>
    <td><%=h contact.contact_data %></td>
    <td><%=h contact.created_userid %></td>
    <td><%=h contact.last_updt_userid %></td>
    <td><%= link_to 'Show', contact %></td>
    <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
    <td><%= link_to 'Destroy', contact, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New contact', new_contact_path %>