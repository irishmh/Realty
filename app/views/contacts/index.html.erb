<h1>Listing contacts</h1>
<title>Contacts: <%= controller.action_name %></title>
<%= stylesheet_link_tag 'scaffold', 'realty' %>

<table>
  <tr>
    <th>Contact type</th>
    <th>Contact data</th>
    <th>Created userid</th>
    <th>Last updt userid</th>
  </tr>

<% @contacts.each do |contact| %>
  <tr>
    <!--td><%=h contact.contact_type %></td-->
    <td><%=h contact.contact_data %></td>
    <td><%= link_to 'Show', contact %></td>
    <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
    <td><%= link_to 'Destroy', contact, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New contact', new_contact_path %>