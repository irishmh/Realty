<h1>New contact_type</h1>
<title>Contact Type: <%= controller.action_name %></title>
<%= stylesheet_link_tag 'scaffold', 'realty' %>

<% form_for(@contact_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :contact_type_cd %><br />
    <%= f.text_field :contact_type_cd %>
  </p>
  <p>
    <%= f.label :contact_type_desc %><br />
    <%= f.text_field :contact_type_desc %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', contact_types_path %>
<br /><br />
<b><%= link_to 'Return to homepage...', properties_path %></b>