<% form_for(@property_price) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :property_id %><br />
    <%= f.text_field :property_id %>
  </p>
  <p>
    <%= f.label :property_price %><br />
    <%= f.text_field :property_price %>
  </p>
  <p>
    <%= f.label :created_userid %><br />
