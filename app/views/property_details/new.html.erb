  <p>
    <%= f.label :property_id %><br />
    <%= f.text_field :property_id %>
  </p>
  <p>
    <%= f.label :pic %><br />
    <%= f.text_field :pic %>
  </p>
  <p>
    <%= f.label :pic_seq %><br />
