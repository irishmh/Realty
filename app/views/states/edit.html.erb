<h1>Edit State</h1>
<title>State: <%= controller.action_name %></title>
<%= stylesheet_link_tag 'scaffold', 'realty' %>

<% form_for(@state) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :state_cd %><br />
    <%= f.text_field :state_cd %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @state %> |
<%= link_to 'Back', states_path %>