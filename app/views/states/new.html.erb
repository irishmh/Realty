<h1>New state</h1>

<% form_for(@state) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :state_cd %><br />
    <%= f.text_field :state_cd %>
  </p>
  <p>
    <%= f.label :created_userid %><br />
    <%= f.text_field :created_userid %>
  </p>
  <p>
    <%= f.label :last_updt_userid %><br />
    <%= f.text_field :last_updt_userid %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', states_path %>