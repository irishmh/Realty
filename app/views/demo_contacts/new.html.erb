<h1>New Demo Contact</h1>
<title>Demo Contact: <%= controller.action_name %></title>
<%= stylesheet_link_tag 'scaffold', 'realty' %>


<% form_for(@demo_contact) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :demo_id %><br />
    <%= f.select(:demo_id, Demo.all.collect{|t| [t.name, t.id]}, {:include_blank => true}) %>
    </select>
  </p>
  <p>
    <%= f.label :preferred_seq %><br />
    <%= f.text_field :preferred_seq %>
  </p>
  <p>
    <%= f.label :contact_type_id %><br />
    <%= f.select(:contact_id, Contact.all.collect{|t| [t.contact_data, t.id]}, {:include_blank => true}) %>
    </select>
  </p>
  <p>
    <%= f.label :contact_data %><br />
    <%= f.text_field :contact_data %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', demo_contacts_path %>
<br /><br />
<b><%= link_to 'Return to homepage...', properties_path %></b>