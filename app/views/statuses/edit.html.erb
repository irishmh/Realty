<h1>Edit Status</h1>

<% form_for(@status) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :status_cd %><br />
    <%= f.text_field :status_cd %>
  </p>
  <p>
    <%= f.label :status_desc %><br />
    <%= f.text_field :status_desc %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @status %> |
<%= link_to 'Back', statuses_path %>